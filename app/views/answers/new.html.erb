<!-- <h1>Please answer these questions</h1>  -->
<%#= form_with url: answers_path, scope: :questions do |f| %>
  <%# @questions.each do |question| %>
    <%#= f.label question.question %>
    <%#= f.check_box question.id %>
  <%# end %>
  <%#= submit_tag "Submit" %>
<%# end %>

<div class="test-container">
  <form action="" class="test">
    <div class="row">
      <div class="col-md-8 offset-md-2">
        <%= render "questions_carousel", questions: @questions %>
        <div class="test-buttons">
          <button type="button" id="btn-prev" class="btn btn-primary"><i class="fas fa-angle-left"></i></button>
          <button type="submit" class="questions-submit btn btn-primary">submit</button>
          <button type="button" id="btn-next" class="btn btn-primary"><i class="fas fa-angle-right"></i></button>
        </div>
      </div>
    </div>
  </form>

